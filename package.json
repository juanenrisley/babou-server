{
  "name": "babou-server",
  "version": "0.0.1",
  "main": "lib/index.js",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "npm run build:clean && babel lib -w -d dist",
    "build:clean": "rm -rf dist",
    "dev": "nodemon dist/index.js",
    "eslint:fix": "eslint --fix \"/**/*.js\"",
    "format": "prettier --write \"/**/*.js\"",
    "serve": "node dist/index.js",
    "start": "run-p -l build dev"
  },
  "dependencies": {
    "graphql": "14.4.2",
    "graphql-yoga": "1.18.0"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.0",
    "@babel/preset-env": "7.5.0",
    "babel-eslint": "10.0.2",
    "eslint": "6.0.1",
    "nodemon": "1.19.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2"
  }
}
